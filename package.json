  "scripts": {
    "build:domain:data": "cross-env NODE_ENV=production DOMAIN=DATA NODE_OPTIONS='--max_old_space_size=20480' webpack build --config config/webpack.prod.js --env OM_MODULE=all",
    "build:domain:application": "cross-env NODE_ENV=production DOMAIN=APPLICATION NODE_OPTIONS='--max_old_space_size=20480' webpack build --config config/webpack.prod.js --env OM_MODULE=all",
    "build:domain:analysis": "cross-env NODE_ENV=production DOMAIN=ANALYSIS NODE_OPTIONS='--max_old_space_size=20480' webpack build --config config/webpack.prod.js --env OM_MODULE=all",
    "build-mobile": "cross-env NODE_ENV=production NODE_OPTIONS='--max_old_space_size=20480' webpack build --config config/webpack.mobile.prod.js --env OM_MODULE=mobileapp",
    "build-share-app": "cross-env NODE_ENV=production NODE_OPTIONS='--max_old_space_size=20480' webpack build --config config/webpack.share.prod.js --env OM_MODULE=all",
    "start:share": "cross-env NODE_ENV=development NODE_OPTIONS='--max-old-space-size=20120' webpack serve --config config/webpack.share.dev.js",
    "analyze": "npm run build -- --analyze",
    "mobile-analyze": "npm run build-mobile -- --analyze",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "start:noRefresh": "cross-env NODE_ENV=development NOREFRESH=true node scripts/rundev.js",
    "start:dev:module": "cross-env NODE_ENV=development NODE_OPTIONS='--max-old-space-size=12288' webpack serve --config config/webpack.dev.js ",
    "test": "jest",
    "build": "node scripts/build.js",
    "envinit": "gulp",
    "preplugin": "npm run build",
    "plugin": "node scripts/genPlugin.js",
    "plugin:sourcemap": "node scripts/build.js && node scripts/genPlugin.js --source=true",
    "sort": "npx sort-package-json",
    "": "cross-env NODE_OPTIONS='--max-old-space-size=10120' webpack serve --config config/webpack.dev.js --env OM_MODULE=license",
    "docs": "npx docsify serve docs -p 44875",
    "splitRouter": "node scripts/genRouter.js",
    "mobileClean": "node scripts/mobileClean.js",
    "mobileRevert": "node scripts/mobileRevert.js",
    "commit": "git-cz",
    "eslint": "eslint -f checkstyle src > eslint.xml || echo eslint ending...",
    "eslint:report": "eslint src/**/*.js* -f json > report.json ||echo eslint ending..",
    "stylelint:report": "stylelint src/**/*.*ss -f json > stylelint.json|| echo stylelint ending...",
    "build:oss": "cross-env UPLOAD_OSS=true npm run build",
    "upload": "node scripts/uploadOSS.mjs",
    "dep:analyze": "node scripts/dep/analyze.js"
  },
